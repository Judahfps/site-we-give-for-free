{
    "version": 2,
    "builds": [
      {
        "src": "index.html",
        "use": "@vercel/static"
      },
      {
        "src": "styles/output.css",
        "use": "@vercel/static"
      },
      {
        "src": "main.js",
        "use": "@vercel/static"
      },
      {
        "src": "assets/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/index.html" }
    ]
  }